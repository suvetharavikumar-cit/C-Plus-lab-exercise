#include <iostream>
using namespace std;
int main()
{
    int rows;
    cout<<" Experiment 1- Half pyramid SUVETHA R"<<endl;
    cout << "Enter number of rows: ";
    cin >> rows;
    for(int i = 1; i <= rows; ++i)
    {
       for(int j = 1; j <= i; ++j) 
       {
            cout << "* "; 
       }
    cout << "\n";
}
return 0; }
