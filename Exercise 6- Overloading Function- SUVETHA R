#include<iostream>
#include<string>
using namespace std;
class calculator
{
    public:
    int add (int a,int b)
    {
        return a+b;
    }
    double add(double a,double b)
    {
        return a+b;
    }
    string add(string a,string b)
    {
        return a+b;
    }
};
int main()
{
    calculator math;
    int result1=math.add(10,9);
    double result2=math.add(10.99,70.86);
    string result3=math.add("Suvetha","Ravikumar");
    cout<<"Exercise3- Overloading function- SUVETHA R"<<endl;
    cout<<"Adding integrs "<<result1<<endl;
    cout<<"Adding float values "<<result2<<endl;
    cout<<"Adding string "<<result3<<endl;
    return 0;
}
